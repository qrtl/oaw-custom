<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

    <record id="view_supplier_stock_tree" model="ir.ui.view">
        <field name="name">view.supplier.stock.tree</field>
        <field name="model">supplier.stock</field>
        <field name="inherit_id"
               ref="supplier_stock.view_supplier_stock_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="attributes">
                <attribute name="groups">stock.group_stock_user</attribute>
            </xpath>
            <xpath expr="//field[@name='partner_loc_id']" position="replace">
                <field name="partner_loc_id"
                       groups="stock.group_stock_user"
                       context="{'default_owner_id': partner_id}"/>
                <field name="partner_loc_id_supplier"
                       options="{'no_quick_create': True, 'no_create_edit': True, 'no_open': True}"
                       groups="model_security_adjust_oaw.group_supplier"/>
            </xpath>
        </field>
    </record>

    <record id="supplier_stock.action_supplier_stock"
            model="ir.actions.act_window">
         <field name="name">Partner Stock</field>
         <field name="res_model">supplier.stock</field>
         <field name="type">ir.actions.act_window</field>
         <field name="view_type">form</field>
         <field name="view_mode">tree</field>
         <field name="context">{}</field>
         <field name="search_view_id" ref="supplier_stock.supplier_stock_search"/>
    </record>

</data>
</openerp>

